<Window x:Class="LedMusic2.OutputConfigurator"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LedMusic2"
        mc:Ignorable="d"
        Title="OutputConfigurator" Height="270" MinHeight="270" Width="400" MinWidth="400">
    <Grid>
        <ListBox HorizontalAlignment="Left" Margin="10,35,0,10" Width="125"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="60"/>
        <Button Content="Remove" HorizontalAlignment="Left" Margin="75,10,0,0" VerticalAlignment="Top" Width="60"/>
        <ContentControl Content="ContentControl" Margin="140,35,10,10"/>
        <TextBlock HorizontalAlignment="Left" Margin="140,10,0,0" TextWrapping="Wrap" Text="Output Type:" VerticalAlignment="Top" Height="20" Padding="0,2,0,0"/>
        <ComboBox Margin="220,8,10,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
