{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "quotemark": [true, "double"],
    "indent": [true, "spaces", 4],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "member-access": false,
    "no-empty": false,
    "no-console": false,
    "object-literal-key-quotes": false
  }
}
